{
  "mcpServers": {
    "sequential-thinking": {
      "type": "stdio",
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-sequential-thinking"
      ]
    },
    "fishit-pipeline": {
      "type": "stdio",
      "command": "java",
      "args": [
        "-jar",
        "${workspaceFolder}/tools/mcp-server/build/libs/mcp-server-1.0.0-all.jar"
      ],
      "env": {
        "XTREAM_URL": "${env:COPILOT_MCP_XTREAM_URL}",
        "XTREAM_USER": "${env:COPILOT_MCP_XTREAM_USER}",
        "XTREAM_PASS": "${env:COPILOT_MCP_XTREAM_PASS}",
        "TELEGRAM_API_ID": "${env:COPILOT_MCP_TELEGRAM_API_ID}",
        "TELEGRAM_API_HASH": "${env:COPILOT_MCP_TELEGRAM_API_HASH}"
      }
    }
  }
}
